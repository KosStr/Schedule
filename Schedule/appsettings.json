{
  "ConnectionStrings": {
    "ScheduleConnection": "#{ConnectionStrings.ScheduleConnection}"
  },

  "JwtSettings": {
    "Issuer": "#{JwtSettings.Issuer}",
    "Audience": "#{JwtSettings.Audience}",
    "Key": "#{JwtSettings.Key}",
    "Lifetime": "#{JwtSettings.Lifetime}",
    "RefreshLifetime": "#{JwtSettings.RefreshLifetime}"
  },

  "Client": {
    "BaseApiUrl": "#{Client.BaseApiUrl}",
    "BaseUrl": "#{Client.BaseUrl}"
  },

  "EmailSettings": {
    "From": "#{EmailSettings.From}",
    "SmtpSettings": {
      "Host": "#{SmtpSettings.Host}",
      "Port": "#{SmtpSettings.Port}",
      "SenderCredentials": {
        "User": "#{SenderCredentials.User}",
        "Password": "#{SenderCredentials.Password}"
      }
    }
  },

  "AzureBlobStorageOptions": {
    "AccessKey": "#{AzureBlobStorageOptions.AccessKey}",
    "Container": "#{AzureBlobStorageOptions.Container}"
  }
}